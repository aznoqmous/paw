{
    "name": "paw",
    "version": "1.0.0",
    "description": "```js\r import SWrapper from './sw-wrapper'",
    "main": "dist/main.js",
    "scripts": {
        "paw:manifest": "node .scripts/build-manifest.js",
        "paw": "node .scripts/build-manifest.js && webpack --config paw.config-dev.js --mode production",
        "paw:dev": "node .scripts/build-manifest.js && webpack --config paw.config-dev.js --mode development",
        "paw:watch": "node .scripts/build-manifest.js && webpack --config paw.config-dev.js --mode development --watch",
        "paw:crawl": "node .scripts/crawl.js",
        "postinstall": "node .scripts/install.js",
        "preuninstall": "node .scripts/uninstall.js"
    },
